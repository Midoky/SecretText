<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uuu"
    android:versionCode="1"
    android:versionName="1.0" >
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
	<uses-permission android:name="android.permission.SEND_SMS"/>
	<uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
	<uses-permission android:name="android.permission.READ_CONTACTS"/>
	<uses-permission android:name="android.permission.READ_PHONE_STATE" />
	
    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/wings"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
             android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".SendMessage" >
            <category android:name="android.intent.category.DEFAULT" />
        </activity>
        <activity 
            android:name="com.example.uuu.Loading">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyDatabaseHelper" >
        </activity>
         <activity android:name=".Login" >
        </activity>
        <activity android:name=".Login_notification" >
        </activity>
         <activity android:name=".SmsList" >
         </activity>
         <activity android:name=".AddContact" >
        </activity>
         <activity android:name=".Settings" >
        </activity>
         <activity android:name=".Ctts" >
        </activity>
         <activity android:name=".CttsAdapter" >
        </activity>
         <activity android:name=".CttsList" >
        </activity>
        <activity android:name=".Msg" >
        </activity>
        <activity android:name=".MsgAdapter" >
        </activity>
        <activity android:name=".MsgList" >
        </activity>
		<activity android:name=".About" >
        </activity>
        <service android:name  =".MsgService">
        </service>          	
        <receiver android:name=".BootCompleteReceiver">
            <intent-filter >
                <action android:name="android.intent.action.BOOT_COMPLETED"></action>
            </intent-filter>
        </receiver>
    </application>
</manifest>
